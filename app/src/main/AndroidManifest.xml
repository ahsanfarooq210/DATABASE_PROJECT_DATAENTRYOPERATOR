        android:theme="@style/AppTheme"
        android:label="" />
    <activity android:name=".ShopActivities.Show_Shop_Activity"
        android:theme="@style/AppTheme"
        android:label=""></activity>
    <activity
        android:name=".ShopActivities.delete_shops_activity"
        android:theme="@style/AppTheme"
        android:label="" />
    <activity
        android:name=".ShopActivities.Shop_details_activity"
